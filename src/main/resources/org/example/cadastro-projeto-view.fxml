<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.CadastroProjetoViewController">
   <children>
      <Label layoutX="198.0" layoutY="14.0" text="Cadastro de Novo Projeto">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="152.0" layoutY="70.0" text="Nome do Projeto" />
      <TextField fx:id="nomeProjetoTextField" layoutX="266.0" layoutY="66.0" prefHeight="25.0" prefWidth="174.0" />
      <Label layoutX="205.0" layoutY="111.0" text="Cliente" />
      <TextField fx:id="clienteTextField" layoutX="266.0" layoutY="107.0" prefHeight="25.0" prefWidth="174.0" />
